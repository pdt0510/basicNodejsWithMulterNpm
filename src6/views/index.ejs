<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Node.js basic</title>
    <link rel="stylesheet" href="css/style.css" />
  </head>
  <body>
    <!-- 2ms41ss -->
    <div class="topnav">
      <a class="active" href="#home">Home</a>
      <a href="#news">News</a>
      <a href="#contact">Contact</a>
      <a href="#about">About</a>
    </div>

    <!-- 10ms24ss -->
    <div>
      <h2>User list</h2>
      <table id="customers">
        <tr>
          <th>Id</th>
          <th>First name</th>
          <th>Last name</th>
          <th>Action</th>
        </tr>
        <!-- 15ms21ss -->
        <h3>users (%): <% dataUser.length%> peoples</h3>
        <h3>users (%=): <%= dataUser.length%> peoples</h3>
        <% for(let i=0; i < dataUser.length; i++) { %>
        <tr>
          <td><%= dataUser[i].id %></td>
          <td><%= dataUser[i].firstName %></td>
          <td><%= dataUser[i].lastName %></td>
          <td>
            <!-- 31ms36ss -->
            <a href="detail/user/<%= dataUser[i].id%>" target="_blank">
              Details
            </a>
            <a href="edit/user/<%= dataUser[i].id%>" target="_blank">Edit</a>
            <a href="delete/user/<%= dataUser[i].id%>" target="_blank">
              Delete
            </a>
          </td>
        </tr>
        <% } %>
      </table>
    </div>
  </body>
</html>
